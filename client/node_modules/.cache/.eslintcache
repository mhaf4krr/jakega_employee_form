twitter\"\n              onChange={(e) => {\n                handleFormFill(\"social_information\", \"twitter\", e.target.value);\n              }}\n              value={form[\"twitter\"]}\n            />\n\n            <Form.Field\n              control={Form.Input}\n              label=\"WhatsApp Number\"\n              name=\"whatsapp\"\n              onChange={(e) => {\n                handleFormFill(\n                  \"social_information\",\n                  \"whatsapp\",\n                  e.target.value\n                );\n              }}\n              value={form[\"whatsapp\"]}\n            />\n          </Form.Group>\n\n          <div className={styles[\"btn_wrapper\"]}>\n            <Button.Group>\n              <Button\n                size=\"medium\"\n                color=\"yellow\"\n                onClick={(e) => {\n                  e.preventDefault();\n                  decrementStep();\n                }}\n              >\n                BACK: (6). MOBILE NUMBERS\n              </Button>\n              <Button.Or />\n              <Button\n                size=\"medium\"\n                positive\n                disabled={!validateInputs(form)}\n                onClick={(e) => {\n                  e.preventDefault();\n                }}\n              >\n                (6). LEGAL INFORMATION : NEXT\n              </Button>\n            </Button.Group>\n          </div>\n        </Form>\n      </div>\n    </div>\n  );\n}\n","/home/hyder/Desktop/Form_Project/client/src/components/Vehicles_Info/VehicleTable/Table.js",[],"/home/hyder/Desktop/Form_Project/client/src/components/Educational_Info/EducationalTable/Table.js",[],"/home/hyder/Desktop/Form_Project/client/src/components/Account_Numbers_information/AccountTable/Table.js",[],"/home/hyder/Desktop/Form_Project/client/src/utils/DepartmenListData.js",[],"/home/hyder/Desktop/Form_Project/client/src/utils/Modal/Modal.js",[],"/home/hyder/Desktop/Form_Project/client/src/utils/NationalityData.js",